{
  "name": "gravitee-dev-repos",
  "version": "0.0.9999",
  "description": "A CLI Tool to manage Circle CI Orb Projects Lifecycle.",
  "main": "src/main.ts",
  "dependencies": {
    "@types/shelljs": "^0.8.8",
    "@types/yargs": "^15.0.6",
    "directory-tree": "^2.2.4",
    "dotenv": "^8.2.0",
    "path": "^0.12.7",
    "shelljs": "^0.8.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "eledoc": "^0.2.1",
    "typedoc": "^0.18.0"
  },
  "scripts": {
    "test": "echo 'no tests implemented yet'",
    "dev": "npm run build && ./node_modules/ts-node/dist/bin.js ./src/main.ts",
    "start": "node ./dist/main.js",
    "doc": "./node_modules/typedoc/bin/typedoc --out docs/ ./src --theme node_modules/eledoc/bin/default/",
    "compile": "tsc ",
    "build": "npm run compile && npm run test && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://<%= Orb_Git_Repo_Ssh %>"
  },
  "keywords": [
    "circleci",
    "typescript"
  ],
  "author": "Jean-Baptiste Lasselle <jean.baptiste.lasselle@gmail.com>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "<%= Orb_Git_Repo %>/issues"
  },
  "homepage": "<%= Orb_Git_Repo %>#README.md"
}
